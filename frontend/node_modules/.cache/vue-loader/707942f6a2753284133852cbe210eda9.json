rmData.birthday\"\n                        input-class=\"form-control bg-white\"\n                        format=\"yyyy-MM-dd\"\n                      ></datepicker>\n                    </div>\n                  </div>\n                  <!-- end form-group -->\n                  <!-- begin form-group -->\n                  <div class=\"form-group row m-b-10\">\n                    <label class=\"col-md-3 col-form-label text-md-right\">\n                      Mobile Phone Number\n                      <span class=\"text-danger\">*</span>\n                    </label>\n                    <div class=\"col-md-6\">\n                      <input\n                        type=\"text\"\n                        name=\"phone number\"\n                        placeholder=\"Mobile Phone Number\"\n                        class=\"form-control\"\n                        v-validate=\"{ required: true, regex:/^1[34578]\\d{9}$/ }\"\n                        v-bind:class=\"{'is-invalid': errors.has('phone number')}\"\n                        v-model=\"formData.phone_num\"\n                      >\n                      <span style=\"color: red !important;\">{{ errors.first('phone number') }}</span>\n                    </div>\n                  </div>\n                  <!-- end form-group -->\n                  <!-- begin form-group -->\n                  <div class=\"form-group row m-b-10\">\n                    <label class=\"col-md-3 col-form-label text-md-right\">\n                      Passport Number\n                      <span class=\"text-danger\">*</span>\n                    </label>\n                    <div class=\"col-md-6\">\n                      <input\n                        type=\"text\"\n                        class=\"form-control\"\n                        placeholder=\"Passport Number\"\n                        name=\"passport\"\n                        v-validate=\"{ required: false, regex:/^\\d{8,9}$/ }\"\n                        v-bind:class=\"{'is-invalid': errors.has('passport')}\"\n                        v-model=\"formData.passport_num\"\n                      >\n                      <span style=\"color: red !important;\">{{ errors.first('passport') }}</span>\n                    </div>\n                  </div>\n                  <!-- begin form-group -->\n                  <div class=\"form-group row m-b-10\">\n                    <label class=\"col-md-3 col-form-label text-md-right\">Address</label>\n                    <div class=\"col-md-9\">\n                      <textarea\n                        class=\"form-control m-b-10\"\n                        rows=\"3\"\n                        placeholder=\"Address\"\n                        v-model=\"formData.address\"\n                      ></textarea>\n                    </div>\n                  </div>\n                  <!-- end form-group -->\n                </div>\n                <!-- end col-8 -->\n              </div>\n              <!-- end row -->\n            </fieldset>\n            <!-- end fieldset -->\n          </b-card>\n        </tab-content>\n        <tab-content title=\"Select Insurance Project\">\n          <b-card class=\"mb-3\" style=\"background-color: rgba(178, 250, 228, 0.068) !important;\">\n            <!-- begin step-2 -->\n            <div id=\"step-2\">\n              <!-- begin fieldset -->\n              <fieldset>\n                <!-- begin row -->\n                <div class=\"row\">\n                  <!-- begin col-8 -->\n                  <legend\n                    class=\"no-border f-w-700 p-b-0 m-t-0 m-b-20 f-s-16 text-inverse\"\n                  >Please select one proper project (product)</legend>\n                  <table class=\"table m-b-0\">\n                    <thead>\n                      <tr>\n                        <th>#</th>\n                        <th>Product Name</th>\n                        <th>Project Information</th>\n                        <th>Product Description</th>\n                      </tr>\n                    </thead>\n                    <tbody>\n                      <tr>\n                        <td>1</td>\n                        <td>\n                          <div>1 Time Insurance</div>\n                          <div>\n                            <br>\n                            <button\n                              type=\"button\"\n                              class=\"btn btn-default m-r-5 m-b-5\"\n                              href=\"javascript:;\"\n                              v-b-modal.modalDialog\n                            >Detail</button>\n                          </div>\n                        </td>\n                        <td>\n                          <div class=\"form-check\">\n                            <p>\n                              <input\n                                class=\"form-check-input\"\n                                type=\"radio\"\n                                name=\"default_radio\"\n                                id=\"defaultRadio\"\n                                value=\"1-1-90\"\n                                v-on:click=\"checkProjectValue\"\n                              >\n                              <label\n                                class=\"form-check-label\"\n                                for=\"defaultRadio\"\n                              >Project 1 - (€90)</label>\n                            </p>\n                          </div>\n                          <div class=\"form-check\">\n                            <p>\n                              <input\n                                class=\"form-check-input\"\n                                type=\"radio\"\n                                name=\"default_radio\"\n                                id=\"defaultRadio\"\n                                value=\"1-2-180\"\n                                v-on:click=\"checkProjectValue\"\n                              >\n                              <label\n                   